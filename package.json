  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ForestAdmin/forest-express-sequelize.git"
  },
  "main": "dist/index.js",
  "dependencies": {
    "babel-runtime": "7.0.0-beta.3",
    "bluebird": "2.9.25",
    "forest-express": "5.4.1",
    "http-errors": "1.6.1",
    "lodash": "4.17.13",
    "moment": "2.19.4",
    "semver": "5.4.1"
  },
  "devDependencies": {
    "@forestadmin/devops": "2.0.0",
    "babel-cli": "7.0.0-beta.3",
    "babel-plugin-transform-runtime": "7.0.0-beta.3",
    "babel-preset-env": "7.0.0-beta.3",
    "babel-register": "7.0.0-beta.3",
    "dotenv": "6.2.0",
    "eslint": "6.7.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.0.4",
    "jest": "24.9.0",
    "mysql2": "1.6.5",
    "onchange": "6.0.0",
    "pg": "7.10.0",
    "pre-commit": "1.2.2",
    "sequelize": "5.10.1",
    "sequelize-fixtures": "0.10.0",
    "simple-git": "1.65.0"
  },
  "scripts": {
    "build": "./node_modules/babel-cli/bin/babel.js src --out-dir dist && echo '\n\\033[0;34m[+] \\033[0;32mBuild done\\033[0m'",
    "build:watch": "onchange 'src/**/*.js' -i -- yarn build",
    "lint": "./node_modules/eslint/bin/eslint.js .eslint-bin scripts src test",
    "pre-commit": "node ./.eslint-bin/pre-commit-hook.js",
    "readme:update-coverage": "yarn test:coverage && node ./scripts/update-coverage.js",
    "release": "yarn build && node ./scripts/release.js",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "pre-commit": [
    "pre-commit"
  ]
}
